  "author": "Ryan Conrad",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "jest": "^27.4.7",
    "rollup": "^2.66.1",
    "typescript": "^4.5.5"
  },
  "files": ["*", "src/*", "benchmark/*", "test/*", "dist/src", "dist/index.js"]
